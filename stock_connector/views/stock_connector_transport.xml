<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record id="stock_connector_transport_tree_view" model="ir.ui.view">
            <field name="name">stock_connector_transport_tree_view</field>
            <field name="model">stock_connector.transport</field>
            <field name="arch" type="xml">
                <tree string="">
                    <field name="name"/>
                    <field name="version"/>
                </tree>
            </field>
        </record>
        <record id="stock_connector_transport_form_view" model="ir.ui.view">
            <field name="name">stock_connector_transport_form_view</field>
            <field name="model">stock_connector.transport</field>
            <field name="arch" type="xml">
                <form string="Transport">
                    <sheet>
                        <group>
                            <field name="name"/>
                            <field name="version"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        <record id="stock_connector_transport_action" model="ir.actions.act_window">
            <field name="name">Warehouse Transports</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">stock_connector.transport</field>
            <field name="view_mode">tree,form</field>
        </record>
    </data>
</openerp>