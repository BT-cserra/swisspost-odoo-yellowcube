<openerp>
	<data>
		<record id="queue_job_form_view_ext" model="ir.ui.view">
			<field name="name">queue.job.form.ext</field>
			<field name="model">queue.job</field>
			<field name="inherit_id" ref="connector.view_queue_job_form"/>
			<field
				name="arch"
				type="xml">
				<data>
					<field
						name="worker_id"
						position="after">
						<field name="revision_info" />
					</field>
				</data>
			</field>
		</record>
		<record id="queue_job_tree_view_ext" model="ir.ui.view">
			<field name="name">queue.job.tree.ext</field>
			<field name="model">queue.job</field>
			<field name="inherit_id" ref="connector.view_queue_job_tree"/>
			<field
				name="arch"
				type="xml">
				<data>
					<field
						name="state"
						position="after">
						<field name="revision_info" />
					</field>
				</data>
			</field>
		</record>
	</data>
</openerp>